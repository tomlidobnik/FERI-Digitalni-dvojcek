city{
    road["Ptujska cesta"]{
        polyline[(1,2),(3,4),(5,6)];
    };
    area["FERI"]{
        polygon[(1,1),(1,2),(2,2),(2,1),(1,1)];
    };
    area["Park"]{
        circle[(4,3),2];
    };
    let $p = (1,1);
    let $q = (2,2);
    
    road["Ljubljanska cesta"]{
        polyline[$p,$q,$p+$q];
    };

    ?{[(1,1),$p,$q,(3,4)],[(1,1),3]};

    let $r = (fst(p),snd(q));
    mark["Center"]{
        $r;
    };
};
